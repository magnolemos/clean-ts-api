{
  "name": "clean-node-api",
  "version": "1.0.0",
  "description": "Estudo de node com TS e Jest",
  "main": "index.js",
  "scripts": {
    "start-mongodb": "mongod --config /Users/magnolemos/Documents/study/mongodb/config/mongod.conf",
    "get-process-mongo": "ps -ef | grep mongo",
    "kill-process-mongodb": "kill {number}",
    "start": "sucrase-node src/main/server.ts",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/magnolemos/clean-ts-api.git"
  },
  "author": "Magno Lemos",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/magnolemos/clean-ts-api/issues"
  },
  "homepage": "https://github.com/magnolemos/clean-ts-api#readme",
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.1.5",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.1",
    "@types/mongodb": "^3.5.14",
    "@types/node": "^13.11.0",
    "@types/supertest": "^2.0.8",
    "@types/validator": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "eslint": "^6.8.0",
    "eslint-config-standard-with-typescript": "^15.0.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "git-commit-msg-linter": "^2.4.2",
    "jest": "^25.2.7",
    "lint-staged": "^10.1.2",
    "sucrase": "^3.13.0",
    "supertest": "^4.0.2",
    "ts-jest": "^25.3.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "-": "0.0.1",
    "bcrypt": "^4.0.1",
    "d": "^1.0.1",
    "express": "^4.17.1",
    "fast-glob": "^3.2.2",
    "husky": "^4.2.3",
    "mongodb": "^3.5.7",
    "validator": "^13.0.0"
  }
}
